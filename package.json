{
  "name": "xfinityapp",
  "version": "1.0.0",
  "license": "0BSD",
  "private": true,
  "main": "index.ts",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "mock-server:start": "node tests/mocks/router/server.js",
    "mock-server:stop": "node -e \"require('./tests/mocks/router/server').stopMockServer();\"",
    "adb:reverse": "adb reverse tcp:8081 tcp:8081",
    "build:dev": "cp .env.dev .env && npx expo build",
    "build:prod": "cp .env.prod .env && npx expo build --release-channel production",
    "build:preview": "npx expo build --release-channel preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:mcp": "testsprite-mcp-plugin generateCodeAndExecute --config testsprite.mcp.json",
    "e2e:build": "detox build",
    "e2e:test": "detox test",
    "e2e:android:debug": "detox test --configuration android.emu.debug",
    "e2e:android:release": "detox test --configuration android.emu.release",
    "e2e:ios:debug": "detox test --configuration ios.sim.debug",
    "e2e:ios:release": "detox test --configuration ios.sim.release"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/ngrok": "^4.1.0",
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "4.5.6",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@sentry/react-native": "~6.14.0",
    "@supabase/supabase-js": "2.50.0",
    "@tanstack/react-query": "^5.81.5",
    "axios": "1.9.0",
    "dotenv": "^17.0.0",
    "expo": "^53.0.22",
    "expo-camera": "~16.1.9",
    "expo-constants": "^17.1.6",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "^18.1.11",
    "expo-font": "~13.3.2",
    "expo-linear-gradient": "~14.1.5",
    "expo-media-library": "^17.1.7",
    "expo-sharing": "^13.1.5",
    "expo-updates": "^0.28.15",
    "lucide-react-native": "0.513.0",
    "node-html-parser": "7.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "^0.79.5",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-network-info": "^5.2.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "sonner-native": "0.20.0",
    "tough-cookie": "^5.1.2",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@testsprite/testsprite-mcp": "^0.0.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@types/ws": "^8.18.1",
    "cors": "^2.8.5",
    "detox": "^20.40.2",
    "expo-doctor": "^1.13.3",
    "express": "^5.1.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-test-renderer": "^19.0.0",
    "sharp": "^0.34.2",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3",
    "ws": "^8.18.3"
  }
}
